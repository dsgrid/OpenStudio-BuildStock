  <class_name>PrbsScheduleMakerOS</class_name>
  <display_name>PrbsScheduleMakerOS</display_name>
  <description>OS version of measure that makes a PRBS DR event schedule</description>
  <modeler_description>OS version of measure that makes a PRBS DR event schedule</modeler_description>
  <arguments>
    <argument>
      <name>dr_event_schedule</name>
      <display_name>DR Event Schedule Name</display_name>
      <description>Name of the schedule to use for setting the EMS code.</description>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>dr_event_hvac</default_value>
    </argument>
    <argument>
      <name>minimum_value</name>
      <display_name>Minimum Value for PRBS Signal</display_name>
      <description>The minimum value to use. Typically -1 or 0.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>maximum_value</name>
      <display_name>Maximum Value for PRBS Signal</display_name>
      <description>The maximum value to use. Typically 0 or 1.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>timestep</name>
      <display_name>Timestep (min)</display_name>
      <description>By default, the measure pulls the simulation timestep from the OpenStudio model. This argument provides the option to override that value.</description>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>minimum_hold_duration</name>
      <display_name>Minimum Hold Duration of DR State (min)</display_name>
      <description>Specifies the minimum hold duration of the DR state.  Cannot be shorter than ((1 / Nyquist frequency) = 2 * timestep); otherwise, input will be overridden.</description>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>maximum_hold_duration</name>
      <display_name>Maximum Hold Duration of DR State (min)</display_name>
      <description>Specifies the maximum hold duration of the DR state.  Cannot be longer than 3 hrs (180 min); otherwise, input will be overridden.</description>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
  </arguments>
